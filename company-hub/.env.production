# Campus Hub Production Environment Variables

# Application
NODE_ENV=production
VITE_API_BASE_URL=http://localhost:8000/api
VITE_APP_NAME=Campus Hub
VITE_APP_VERSION=1.0.0

# Face Recognition Settings
VITE_FACE_RECOGNITION_ENABLED=true
VITE_FACE_CONFIDENCE_THRESHOLD=0.6
VITE_MAX_FACE_IMAGES=3

# Resume Analysis Settings
VITE_RESUME_ANALYSIS_ENABLED=true
VITE_MAX_FILE_SIZE=10485760
VITE_SUPPORTED_FILE_TYPES=pdf,doc,docx,txt

# Security
VITE_ENABLE_CORS=true
VITE_ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME_UPDATES=true
